{
  "name": "@brugarolas/swing",
  "description": "A swipeable cards interface. The swipe-left/swipe-right for yes/no input. As seen in apps like Jelly and Tinder.",
  "version": "3.2.0",
  "author": "andres-brugarolas",
  "dependencies": {
    "hammerjs": "^2.0.8",
    "rebound": "^0.1.0",
    "sister": "^3.0.2",
    "prefix": "^1.0.0"
  },
  "peerDependencies": {
    "core-js": "3.1.x",
    "@babel/runtime-corejs3": "7.4.x"
  },
  "devDependencies": {
    "core-js": "^3.1.4",
    "@babel/core": "^7.4.4",
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime-corejs3": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^7.6.0",
    "eslint-config-canonical": "^22.0.0",
    "husky": "^2.3.0",
    "raf": "^3.4.1",
    "mocha": "^6.1.4",
    "chai": "^4.2.0",
    "jsdom": "^15.1.0",
    "sinon": "^7.3.2"
  },
  "keywords": [
    "swipe",
    "cards",
    "swipeable"
  ],
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "dist/Stack.js",
    "dist/Card.js",
    "dist/Direction.js",
    "dist/utilities.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brugarolas/swing"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./dist --copy-files",
    "lint": "eslint ./src ./test",
    "test": "mocha --require @babel/register --exit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  }
}
